sno=int(input(‘enter student no:’)
sname=input(‘enter student name:’)
sgroup=input(‘enter student group’)
s1=int(input(‘maths marks:’))
s2=int(input(‘physics marks:’))
s3=int(input(‘ai marks:’))
s4=int(input(‘english marks:’))
s5=int(input(‘telugu marks:’))
s6=int(input(‘analytical skills marks:’))
s7=int(input(‘ online business marks:’))
if s1>=35 and s2>=35 and s3>=35 and s4>=30 and s5>=35 and s6>=30 and s7>=35:
print(‘pass’)
else:
print(‘fail’)
